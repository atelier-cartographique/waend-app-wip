{
    "compilerOptions": {
        "project": "./",
        "outDir": "../js",
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es6",
        "alwaysStrict": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitUseStrict": false,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictNullChecks": true,
        "removeComments": true,
        "sourceMap": false
    },
    "exclude": [
        "commands",
        "render0",
        "embed",
        "view"
    ]
}